<!--ユーザー一覧-->
<table>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.user_name %></td>
        <td><%= user.email %></td>
        <% if user.is_deleted == false %>
          <td class="text">有効</td>
        <% else %>
          <td class="text">退会済</td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>